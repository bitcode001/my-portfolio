---
const GAN_ID = import.meta.env.GAN_ID
---

<script type="text/partytown" async src="https://www.googletagmanager.com/gtag/js?id=${GAN_ID}"
></script>
<script type="text/partytown" define:vars={{ GI: GAN_ID }}>
	window.dataLayer = window.dataLayer || []
	function gtag() {
		dataLayer.push(arguments)
	}
	gtag('js', new Date())
	gtag('config', GI)
</script>
